}
.nav-pills > li > a {
	color: #58585A;
}

ul.nav.nav-pills.nav-stacked{
	background: #EEEFEF;
	/*padding-left: 22%;
	padding-right: 22%;*/
	font-size: 13px;

	text-align: center;
}

.nav > li > a:hover, .nav > li > a:focus{
	color: #9075B5;
	/*text-decoration: underline;*/
	background: none;
}
.nav-pills > li.active > a{
	background: none!important;
	color: #9075B5!important;

}
