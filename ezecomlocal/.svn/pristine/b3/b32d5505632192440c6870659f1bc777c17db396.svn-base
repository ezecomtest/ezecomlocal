<script type="text/javascript">
  

  function display_camera(rtmp,name,secure){
    stream_camera(rtmp,name,secure);
  }

  function stream_camera(rtmp,name,secure){
    document.getElementById("stream_url").value = rtmp;
    document.getElementById("secure").value = secure;
   //  var name = document.getElementById("name").value;
    set_session(rtmp,secure,name);
    document.getElementById("title_camera").innerHTML = "";
    document.getElementById("title_camera").innerHTML = name;
    play_camera();
  }

  function set_session(rtmp,secure,name){
   // alert(name);
     var base_url= "<?php echo base_url(); ?>";
            $.ajax({
                type:"POST",
                url: base_url+ "set_session_camera",
                data:{rtmp:rtmp,token:secure,camera_name:name},
                success:function(dta)
                {
                  window.location.reload();
                }
            });
  }

   $(function() {
        $(this).bind("contextmenu", function(e) {
            e.preventDefault();
        });
    }); 
</script>
