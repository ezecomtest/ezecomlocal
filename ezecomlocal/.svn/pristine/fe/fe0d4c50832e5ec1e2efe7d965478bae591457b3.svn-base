/* Custom Style: payment options */
.rl_tabs.top > ul.nav > li.payment-tap > a {
    -webkit-transition-duration: 0s;
    -moz-transition-duration: 0s;
    -o-transition-duration: 0s;
    transition-duration: 0s;
    font-size: 12px;
    font-weight: 500;
    background-color: #DBDCDD;
    margin: 0 10px 15px!important;
    border-radius: 50%;
    width: 102px !important;
    height: 102px !important;
    padding: 30px 20px 20px 20px;
}
.rl_tabs.top > ul.nav > li.payment-tap > a:hover {
  background-color: #7961a9;
  color: #fff;
  border-radius: 50%;
    width: 105px !important;
    height: 105px !important;
}
.rl_tabs.top > ul.nav > li.payment-tap.active > a,
.rl_tabs.top > ul.nav > li.payment-tap.active > a:hover {
    border-radius: 50%;
    width: 105px !important;
    height: 105px !important;
    background-color: #7961a9;
}
.rl_tabs>ul.nav-tabs>li.active>a:focus, .rl_tabs>ul.nav-tabs>li>a:focus{
    border: none !important;
}
.rl_tabs.top>ul.nav-tabs>li.active>a:focus, .rl_tabs.top>ul.nav-tabs>li>a:focus{
    box-shadow: none;
}

/* Custom Style: tap contact us */
.rl_tabs.top > ul.nav > li.contact-tap > a {
    -webkit-transition-duration: 0s;
    -moz-transition-duration: 0s;
    -o-transition-duration: 0s;
    transition-duration: 0s;
    font-size: 16px;
    background-color: #fff;
    width: 190px;
    margin: 0 10px 15px!important;
}
.rl_tabs.top > ul.nav > li.contact-tap > a:hover {
  background-color: #7961a9;
  color: #fff;
}
.rl_tabs.top > ul.nav > li.contact-tap.active > a,
.rl_tabs.top > ul.nav > li.contact-tap.active > a:hover {
  
  background-color: #7961a9;
}

/**For FAQ**/
.faqheading{
    background-repeat: no-repeat;
    background-position: 6px center;
    color: #666 !important;
    font-weight:bold !important;
    cursor: pointer; 
    font-size: 16px;  
    overflow: hidden; 
    padding: 10px 0 10px 38px; 
    position: relative;  
    width: 90%;
    margin:0 !important;
}
.q1{
    background-image: url(../images/list-num/1.png);
}
.q2{
	background-image: url(../images/list-num/2.png);
}
.q3{
	background-image: url(../images/list-num/3.png);
}
.q4{
	background-image: url(../images/list-num/4.png);
}
.q5{
	background-image: url(../images/list-num/5.png);
}
.q6{
	background-image: url(../images/list-num/6.png);
}
.q7{
	background-image: url(../images/list-num/7.png);
}
.q8{
	background-image: url(../images/list-num/8.png);
}
.q9{
	background-image: url(../images/list-num/9.png);
}
.q10{
	background-image: url(../images/list-num/10.png);
}

.faqbody{
	
	padding:20px 0 20px 38px;
	
}

.faqbody ul li{
	list-style:lower-alpha;
	padding:5px;
}
.faqheading-icon{
    clear: none;
    float: left;
    height: 22px;
    left: 0;
    position: relative;
    top: -3px;
    width: 30px;
}
.faqbody #email-table tr td{
    padding: 10px !important;
}

/**For Module ***/
.module-title{
    font-weight: 600;
}
.module-ct > ul.archive-module {
    list-style: none;
    margin: 0;
    padding: 0;
}
.module-ct > ul.archive-module .archive-a {
    font-size: 16px;
    font-family: "Raleway", sans-serif;
    font-size: 16px;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.blog-item .article-intro {
  margin-bottom: 20px;
}
.item-page .tags {
    border-bottom: 1px solid #eeeeee;
    margin-bottom: 20px;
    border-top: 1px solid #eeeeee;
    padding: 10px 0;
}
.item-page .tags span {
    display: inline-block;
    margin-bottom: 10px;
}
.item-page .tags span a,.item-page .tags span a:hover {
    background: #eeeeee;
    border: 1px solid #eeeeee;
    border-radius: 3px;
    color: #666666;
    padding: 5px;
}
.article-image-full{
    width: 100%;
    padding:0px 0 15px 0;
}
a:hover, a:focus{
    text-decoration: none;
}
.category-sub-heading{
    color: #fff;
    font-size: 16px;
}
.text-capitalize{
    text-transform: capitalize; 
}
.languageswitcherload .mod-languages{
	text-align: left;
}

